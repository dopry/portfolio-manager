{
  "name": "portfolio-manager",
  "version": "1.5.0",
  "description": "Unofficial CLI Tool and SDK for Energy Star Portfolio Manager",
  "engines": {
    "node": ">=18 <=20"
  },
  "files": [
    "dist/"
  ],
  "bin": {
    "portfolio-manager": "dist/cjs/index.js"
  },
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "test": "mocha",
    "semantic-release": "semantic-release",
    "src-bin": "node --import 'data:text/javascript,import { register } from \"node:module\"; import { pathToFileURL } from \"node:url\"; register(\"ts-node/esm\", pathToFileURL(\"./\"));' ./src/index.ts",
    "build-bin": "node ./dist/index.js"
  },
  "author": {
    "name": "Darrel O'Pry"
  },
  "license": "MIT",
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@types/chai": "^4.3.10",
    "@types/mocha": "^10.0.4",
    "@types/node": "^20.9.1",
    "@types/node-fetch": "^2.6.9",
    "chai": "^4.3.10",
    "mocha": "^10.2.0",
    "node-fetch": "^3.3.2",
    "semantic-release": "^22.0.8",
    "should": "^13.2.3",
    "source-map-support": "^0.5.21",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "commander": "^11.1.0",
    "deepmerge-ts": "^5.1.0",
    "fast-xml-parser": "^4.3.2",
    "type-guards": "^0.15.0"
  },
  "release": {
    "branches": [
      "+([0-9])?(.{+([0-9]),x}).x",
      "main",
      {
        "name": "next",
        "prerelease": true
      }
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/changelog",
        {
          "changelogFile": "CHANGELOG.md"
        }
      ],
      [
        "@semantic-release/npm"
      ],
      [
        "@semantic-release/git",
        {
          "assets": [
            "CHANGES.md",
            "package.json",
            "package-lock.json"
          ],
          "message": "chore(release): ${nextRelease.version}\n\n${nextRelease.notes}"
        }
      ]
    ]
  }
}
